<?php

use Jet\MVC_View;
use Jet\Form_Renderer_Field;

/**
 * @var MVC_View $this
 * @var Form_Renderer_Field $f
 */
$f = $this->getRaw( 'renderer' );

echo
	$f->row()->start() .
		$f->label() .
		$f->container()->start() .
			$f->input() .
			$f->error() .
		$f->container()->end() .
	$f->row()->end();
