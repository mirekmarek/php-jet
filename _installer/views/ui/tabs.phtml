<?php

use Jet\UI_tabs;
use Jet\MVC_View;

/**
 * @var MVC_View $this
 * @var UI_tabs $t
 */
$t = $this->getRaw( 'element' );

$t->setBaseCssClass('nav nav-tabs');
$t->addCustomCssStyle('margin-top: 10px;');
?>
<ul <?=$t->renderTagAttributes()?>>
	<?php foreach( $t->getTabs() as $tab ):
		echo $tab;
	endforeach; ?>
</ul>
