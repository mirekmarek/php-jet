<?php

use Jet\UI_tabsJS_tab;
use Jet\MVC_View;

/**
 * @var MVC_View $this
 * @var UI_tabsJS_tab $t
 */
$t = $this->getRaw( 'element' );

if($t->getIsSelected()) {
	$t->setBaseCssClass('tab-pane fade show active');
} else {
	$t->setBaseCssClass('tab-pane fade');
}

$t->addCustomCssStyle('padding-top: 10px;');
?>
<div <?=$t->renderTagAttributes()?>>

